class Solution{
  public:
    // Should return head of the modified linked list
    Node *sortedInsert(struct Node* head, int data) {
        // Code here
        if(head==NULL){
            return  NULL;
        }
        Node*a=head->next;
        Node*b=head;
        Node*temp=new Node(data);
        if(head->data>=data){
            temp->next=head;
            head=temp;
            return head;
        }
        while(a!=NULL){
           if(a->data>=data){
               b->next=temp;
               temp->next=a;
               break;
           }
           else{
               b=a;
               a=a->next;
           }
        
        }
        if(a==NULL){
            b->next=temp;
        }
        return head;
    }
};
